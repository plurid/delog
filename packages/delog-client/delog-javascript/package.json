        "graphql-tag": "^2.11.0",
        "subscriptions-transport-ws": "^0.9.18"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^15.1.0",
        "@rollup/plugin-node-resolve": "^9.0.0",
        "@types/node": "^14.11.2",
        "@typescript-eslint/eslint-plugin": "^4.2.0",
        "@typescript-eslint/parser": "^4.2.0",
        "eslint": "^7.10.0",
        "jest": "^26.4.2",
        "jest-config": "^26.4.2",
        "react": "^16.13.1",
        "rollup": "^2.28.2",
        "rollup-plugin-typescript2": "^0.27.3",
