{
    "name": "@plurid/delog",
    "version": "0.0.1",
    "description": "Cloud-Native Logger Client",
    "keywords": [
        "delog",
        "client"
    ],
    "author": "ly3xqhl8g9 <ly3xqhl8g9@plurid.com> (https://plurid.com)",
    "license": "MIT",
    "repository": {
        "type": "git",
